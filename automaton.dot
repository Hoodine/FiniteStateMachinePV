digraph finite_automaton {
    rankdir=LR;
    node [shape = circle];

    "" [shape = none];
    "" -> "q0";

    "q0" [shape = circle];
    "q1" [shape = circle];
    "q2" [shape = doublecircle];

    "q0" -> "q1" [label = "a"];
    "q0" -> "q2" [label = "epsilon"];
    "q1" -> "q2" [label = "b"];
    "q2" -> "q0" [label = "a"];
    "q2" -> "q1" [label = "z"];
}
